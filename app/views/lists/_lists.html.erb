<% @lists.each do |list| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to list.title, [@user, list] %>
      </h4>
      <small>
        <%= list.description %> | <%= link_to "Delete", list_path(list.id), method: :delete %>
      </small>
    </div>
  </div>
<% end %>